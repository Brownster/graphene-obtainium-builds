    {
      "id": "audiobookshelf-app",
      "enabled": true,
      "upstream_repo": "advplyr/audiobookshelf-app",
      "release_repo": "Brownster/graphene-obtainium-audiobookshelf",
      "version_source": "latest_tag",
      "checkout_submodules": "false",
      "working_directory": ".",
      "uses_node": true,
      "node_version": "20",
      "uses_ruby": false,
      "ruby_version": "",
      "uses_go": false,
      "go_version_file": "",
      "java_version": "21",
      "android_build_tools": "34.0.0",
      "build_script": "npm ci && npm run generate && npx cap sync && ./android/gradlew assembleRelease -p android --no-daemon",
      "build_artifact_path": "android/app/build/outputs/apk/release/app-release-unsigned.apk",
      "post_signing": true,
      "output_name": "audiobookshelf",
      "package_name": "com.audiobookshelf.app"
    },
